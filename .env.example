# Example Environment Configuration
# Copy to .env and customize for your environment

# Server Configuration
BOTNET_HOST=127.0.0.1
BOTNET_PORT=9999
BOTNET_WEB_PORT=8080

# Security Settings (IMPORTANT: Change these for production)
# Generate encryption key with: python -c "import os, base64; print(base64.b64encode(os.urandom(32)).decode())"
BOTNET_ENCRYPTION_KEY=your_base64_encoded_key_here
BOTNET_ADMIN_PASSWORD=your_secure_password_here

# TLS/SSL Configuration (optional)
# BOTNET_TLS_CERT=/path/to/certificate.pem
# BOTNET_TLS_KEY=/path/to/private_key.pem

# Logging Configuration
BOTNET_LOG_LEVEL=INFO

# Performance Settings
BOTNET_MAX_CONNECTIONS=100
BOTNET_MAX_MESSAGE_SIZE=1048576
